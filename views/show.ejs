<% include partials/header %>

<div class="container">
    <div class="row">
        <div class="col-lg-12, text-center">
            <h3><%= album.name %></h3>
            <h4><%= album.artist %></h4>
            <img src="<%= album.image %>">
            <h5><%= album.about %></h5>
            <h5>Added: <%=album.created.toDateString()%></h4>
        <a class="btn" href="/music/<%= album._id %>/edit">Edit Post</a>
        <form action="/music/<%= album._id %>?_method=DELETE" method="POST">
            <button class="btn btn-primary">Delete Post</button>
        </form>
        <a href="/music">Back to Albums</a>
    </div>
</div>  




<% include partials/footer %>